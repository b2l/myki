
<div class="title">
    <div class="show">
        {{= it.title }}
    </div>
    <input class="edit" type="text" name="title" value="{{= it.title }}" />
</div>

<div class="pull-right">
    <a class="icon icon-save" id="save" href="/page/{{= it.id !== undefined ? it.id + '/' : '' }}save">Enregistrer</a>
    <a class="icon icon-preview" id="show-preview">preview</a>
</div>
<div>
    <input type="file" id="upload"/>
</div>
